	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/PDGS/base/" schemaLocation="../base/base.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/PDGS/center/" schemaLocation="../center/center.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/orbital/" schemaLocation="../../SY/orbital/orbital.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/platform/" schemaLocation="../../SY/platform/platform.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/misc/" schemaLocation="../../SY/misc/misc.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/geographical/" schemaLocation="../../SY/geographical/geographical.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/spatio/" schemaLocation="../../SY/spatio/spatio.xsd"/>
	<xs:import namespace="https://psd-12.sentinel2.eo.esa.int/DICO/12/SY/tile/" schemaLocation="../../SY/tile/tile.xsd"/>
