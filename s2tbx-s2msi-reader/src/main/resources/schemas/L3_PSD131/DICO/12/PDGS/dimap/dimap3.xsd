	</xs:complexType>
	<xs:complexType name="A_L3_PIXEL_LEVEL_QI">
		<xs:annotation>
			<xs:documentation>
				Filenames of the mosaic and classification masks and
				mapping
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="PVI_FILENAME" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Reference to PVI contained in the QI_DATA folder
					</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="L3_TILE_CLASSIFICATION_MASK" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Filename of the classification mask stored in
						the QI data folder
					</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="L3_TILE_MOSAIC_MASK" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						Filename of the mosaic mask stored in the QI
						data folder
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="resolution" type="image:A_BAND_RESOLUTION" default="60"/>
	</xs:complexType>
	<xs:complexType name="A_L3_MOSAIC_CONTENT_QI">
		<xs:annotation>
			<xs:documentation>
				Quantification values of single tile id contributing to
				mosaic mask sythesizing the final L3 product
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>

			<xs:element name="PRODUCT_ID" type="item:Product_ID_2A"></xs:element>
			<xs:element name="TILE_ID" type="item:TILE_ID_2A">
				<xs:annotation>
					<xs:documentation>
						ID of tile used for synthesis
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TILE_PIXEL_COUNT"
				type="misc:A_POSITIVE_LONG">
				<xs:annotation>
					<xs:documentation>
						Count of pixels of tile contributing to mosaic
						mask
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TILE_PIXEL_PERCENTAGE"
				type="misc:A_PERCENTAGE">
				<xs:annotation>
					<xs:documentation>
						Percentage of pixels of tile contributing to
						mosaic mask
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TILE_DATE_TIME"
				type="date_time:AN_UTC_DATE_TIME">
				<xs:annotation>
					<xs:documentation>
						Timestamp of all pxels of tile contributing to
						mosaic mask
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TILE_AOT_MEAN" type="xs:double">
				<xs:annotation>
					<xs:documentation>
						Mean Aerosol Optical Thickness of tile
						contributing to mosaic mask
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TILE_SZA_MEAN"
				type="misc:A_DOUBLE_WITH_DEG_UNIT_ATTR">
				<xs:annotation>
					<xs:documentation>
						Mean Solar Zenith Angle of tile contributing to
						mosaic mask
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>

		<xs:attribute name="tileNumber" type="misc:A_POSITIVE_INT"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="AN_AUXILIARY_DATA_INFO_USERL3">
		<xs:annotation>
			<xs:documentation>Auxiliary Data information L2A on product level</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Aux_Data">
				<xs:annotation>
					<xs:documentation>References to the AUX_DATA items</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="L3_GIPP_LIST"
							type="dimap:A_GIPP_LIST_3" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AN_IMAGE_DATA_INFO_DSL1C_DSL3">
		<xs:annotation>
			<xs:documentation>List of L3 tiles + L1C Geometric and Radiometric info</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Tiles_Information">
				<xs:annotation>
					<xs:documentation>List of the tiles composing the whole datastrip.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Tile_List">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Tile" maxOccurs="unbounded">
										<xs:complexType>
											<xs:attribute name="tileId" type="item:TILE_ID_3" use="required">
												<xs:annotation>
													<xs:documentation>Tile Identifier</xs:documentation>
												</xs:annotation>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
							<xs:unique name="tile_identifier_L3_unique">
								<xs:selector xpath="./Tile_Description/TILE_IDENTIFIER"/>
								<xs:field xpath="@TILE_IDENTIFIER"/>
							</xs:unique>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="metadataLevel" type="dimap:metadataLevel" fixed="Standard"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Sensor_Configuration" type="dimap:A_SENSOR_CONFIGURATION">
				<xs:annotation>
					<xs:documentation>MSI Sensor configuration (Information from MSI telemetry)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Radiometric_Info">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="dimap:A_RADIOMETRIC_DATA_L1C">
							<xs:attribute name="metadataLevel" type="dimap:metadataLevel" fixed="Standard"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Geometric_Info">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="dimap:A_GEOMETRIC_DATA_DSL1C">
							<xs:attribute name="metadataLevel" type="dimap:metadataLevel" fixed="Standard"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Product_Compression" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="COMPRESSION">
							<xs:annotation>
								<xs:documentation>Product Compression</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="NONE"/>
									<xs:enumeration value="LOSSY"/>
									<xs:enumeration value="LOSSLESS"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="metadataLevel" type="dimap:metadataLevel" fixed="Standard"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="A_L3_MOSAIC_LIST">
		<xs:sequence>
			<xs:element name="L3_Mosaic_Content" type="dimap:A_L3_MOSAIC_CONTENT_QI" minOccurs="0" maxOccurs="unbounded"></xs:element>
		</xs:sequence>
		<xs:attribute name="resolution"
			type="image:A_BAND_RESOLUTION">
		</xs:attribute>
	</xs:complexType>
</xs:schema>
